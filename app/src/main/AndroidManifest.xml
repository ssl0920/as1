<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    tools:overrideLibrary="com.google.android.gms.appindexing"
    xmlns:tools="http://schemas.android.com/tools"
    package="ca.sangsooualberta.sangsoo_fueltrack">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        >
        <activity
            android:name=".FuelLogActivity"
            android:label="@string/view_log"
            >
            <intent-filter android:label="@string/app_name">
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".FuelTrackActivity"
            android:label = "@string/log_entry" >
        </activity>
    </application>

</manifest>
